<% if notice %>
<p id ="notice">
  <%= notice %>
</p>
<%end%>
<h1>Your Pragmatic Catalog
<%=	 @tiempo2 %>
</h1>
<% @products.each do |product| %>
<div class="entry">
  <%= image_tag(product.image_url)%>
  <h3><%= product.title  %></h3>
  <%= sanitize(product.description) %>
  <div class="price_line">
    <span class="price">
      <span class="price"><%= number_to_currency(product.price) %></span>
    </span>
  </div>
</div>
<% end %>